{
    "name": "bjelajac/metadata_hex",
    "description": "Extracts metadata from files and inserts it into Drupal nodes",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "Drupal\\metadata_hex\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Belich",
            "email": "developer@davidbelich.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "smalot/pdfparser": "dev-master"
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\metadata_hex\\Unit\\": "tests/src/Unit/"
        }
    },
    "extra": {
        "patches": {},
        "drupal-scaffold": {
            "locations": {
                "web-root": "web"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1@dev"
    },
    "scripts": {
        "phpunit": "./vendor/bin/phpunit --bootstrap web/core/tests/bootstrap.php --configuration web/core/phpunit.xml.dist web/modules/custom"
    }
}
